{
    "data_dir": "./datasets/SNLI/processed/",
    "dataset": "SNLI",
    "log_dir": "logs",
    "log": true,
    "log_idx": 500,
    "use_pretrain": true,
    "pretrained_path": "./datasets/SNLI/emb_glove_300.npy",
    "model_name": "ESIM_CHAR",
    "verbose": true,
    "stats": true,
    "stats_idx": 2,

    "use_char": true,
    "use_pos": true,
    "use_local_feat": false,
    "word_lower": true,
    "char_max": 16,
    "qmax": 80,
    "amax": 60,
    "char_size": 86,
    "pos_size": 48,
    "local_feat_size": -1,
    "char_dim": 64,
    "pos_dim": 10,
    "local_feat_dim":-1,
    "char_kernel_size": 3,

    "embedding_dim": 300,
    "hidden_dim": 300,
    "dropout": 0.2,
    "word_dropout": 0.2,
    "num_classes": 3,
    "freeze_word_embeddings": true,
    "num_layers": 2,
    "k_factor": 150,
    "enhance_mode": "MUL_MIN_CAT",
    "pool_mode": "MAX_AVG",

    "epochs": 64,
    "batch_size": 128,
    "lr": 0.0003,
    "lr_decay": 0.96,
    "decay_patience": 1,
    "max_grad_norm": 5.0,
    "patience": 5
}